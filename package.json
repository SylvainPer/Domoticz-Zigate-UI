{
  "name": "zigate-plugin",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "start": "npm run env -s && ng serve --aot --proxy-config proxy.conf.js",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean --sort --marker extract",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,html,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,html,scss}\"",
    "postinstall": "npm run prettier -s",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@fortawesome/fontawesome-free": "^5.12.0",
    "@ng-bootstrap/ng-bootstrap": "^4.2.2",
    "@ng-select/ng-select": "^2.20.5",
    "@ngx-translate/core": "^11.0.1",
    "@swimlane/ngx-charts": "^12.1.0",
    "@swimlane/ngx-datatable": "^16.0.3",
    "@types/file-saver": "^2.0.1",
    "angular-highcharts": "^8.0.0",
    "bootstrap": "^4.4.1",
    "browserslist": "^4.8.3",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.6.11",
    "file-saver": "^2.0.2",
    "highcharts": "^7.2.1",
    "ng-bootstrap-form-validation": "^4.0.0",
    "ng-push": "^0.2.2",
    "ngx-color-picker": "^8.2.0",
    "ngx-filesaver": "^8.1.0",
    "ngx-gauge": "^1.0.0-beta.11",
    "ngx-json-viewer": "^2.4.0",
    "ngx-perfect-scrollbar": "^7.2.1",
    "ngx-spinner": "^8.1.0",
    "ngx-toastr": "^11.2.1",
    "rxjs": "^6.5.4",
    "subsink": "^1.0.0",
    "toppy": "^2.3.4",
    "web-animations-js": "^2.3.2",
    "zone.js": "^0.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.21",
    "@angular/cli": "^8.3.21",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@biesbjerg/ngx-translate-extract": "^4.2.0",
    "@ngx-rocket/scripts": "^3.0.4",
    "@types/jasmine": "^3.5.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^8.10.59",
    "codelyzer": "^5.2.1",
    "htmlhint": "^0.11.0",
    "https-proxy-agent": "^2.2.4",
    "husky": "^1.3.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.5.1",
    "karma-junit-reporter": "^1.2.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.11.1",
    "protractor": "~5.4.0",
    "stylelint": "~10.0.1",
    "stylelint-config-prettier": "^5.3.0",
    "stylelint-config-recommended-scss": "~3.3.0",
    "stylelint-config-standard": "~18.3.0",
    "stylelint-scss": "~3.6.0",
    "ts-node": "^8.5.4",
    "tslint": "~5.16.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "~3.5.0"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --ignore-path=.prettierignore"
    }
  }
}
